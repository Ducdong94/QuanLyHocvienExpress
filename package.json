{
  "name": "quanlyhocvienexpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon -r dotenv/config ./bin/www dotenv_config_path=environment/.env.development",
    "start:uat": "node -r dotenv/config ./bin/www dotenv_config_path=environment/.env.uat",
    "start:prod": "node -r dotenv/config ./bin/www dotenv_config_path=environment/.env.production",
    "debug": "ndb -r dotenv/config ./bin/www dotenv_config_path=environment/.env.development"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^7.4.0",
    "morgan": "~1.9.1",
    "ndb": "^1.1.5",
    "nodemon": "^3.0.1"
  }
}
